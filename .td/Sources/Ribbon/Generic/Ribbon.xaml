<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dxc="http://schemas.devexpress.com/winfx/2008/xaml/core"
                    xmlns:dxcn="http://schemas.devexpress.com/winfx/2008/xaml/core/internal"
                    xmlns:dxct="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys"
                    xmlns:dxdst="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys"
                    xmlns:dxmvvm="http://schemas.devexpress.com/winfx/2008/xaml/mvvm"
                    xmlns:dxr="http://schemas.devexpress.com/winfx/2008/xaml/ribbon"
                    xmlns:dxri="http://schemas.devexpress.com/winfx/2008/xaml/ribbon/internal"
                    xmlns:dxrt="http://schemas.devexpress.com/winfx/2008/xaml/ribbon/themekeys"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <dxc:BoolInverseConverter x:Key="inverseConverter"/>

    <dxc:BooleanToVisibilityConverter x:Key="boolToVisibilityConverter" Invert="False"/>

    <dxc:BooleanToVisibilityConverter x:Key="invertBoolToVisibilityConverter" Invert="True"/>

    <ControlTemplate x:Key="{dxrt:RibbonPageHeaderLinksControlThemeKey ResourceKey=ControlTemplate}" TargetType="{x:Type dxr:RibbonPageHeaderLinksControl}">
        <ItemsPresenter/>
    </ControlTemplate>

    <ItemsPanelTemplate x:Key="{dxrt:RibbonPageHeaderLinksControlThemeKey ResourceKey=PanelTemplate}">
        <StackPanel Orientation="Horizontal"/>
    </ItemsPanelTemplate>

    <Style x:Key="{dxrt:RibbonThemeKey ResourceKey=RightRepeatButtonStyle}" TargetType="{x:Type RepeatButton}">
        <Setter Property="Template" Value="{DynamicResource {dxrt:RibbonThemeKey ResourceKey=RightRepeatButtonTemplate}}"/>
        <Setter Property="VerticalAlignment" Value="Stretch"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
    </Style>

    <Style x:Key="{dxrt:RibbonThemeKey ResourceKey=LeftRepeatButtonStyle}" TargetType="{x:Type RepeatButton}">
        <Setter Property="Template" Value="{DynamicResource {dxrt:RibbonThemeKey ResourceKey=LeftRepeatButtonTemplate}}"/>
        <Setter Property="VerticalAlignment" Value="Stretch"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
    </Style>

    <ItemsPanelTemplate x:Key="{dxrt:RibbonPageCategoriesPaneThemeKey ResourceKey=PanelTemplate}">
        <dxr:RibbonItemsPanel HorizontalAlignment="Stretch"
                              IsItemsHost="True"
                              VerticalAlignment="Stretch"/>
    </ItemsPanelTemplate>

    <ItemsPanelTemplate x:Key="{dxrt:RibbonPageCategoriesPaneThemeKey ResourceKey=OfficeSlimPanelTemplate}">
        <dxr:RibbonItemsPanelOfficeSlim/>
    </ItemsPanelTemplate>

    <ControlTemplate x:Key="{dxrt:RibbonPageCategoriesPaneThemeKey ResourceKey=ControlTemplate}" TargetType="{x:Type dxr:RibbonPageCategoriesPane}">
        <Grid Name="Root">
            <Grid.RowDefinitions>
                <RowDefinition/>
                <RowDefinition Height="*"/>
                <RowDefinition/>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto"/>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="Auto"/>
            </Grid.ColumnDefinitions>
            <dxr:RibbonPageCategoryHeaderControl x:Name="PART_OriginHeaderControl"
                                                 Grid.ColumnSpan="3"
                                                 Visibility="Hidden"
                                                 Width="0">
                <dxr:RibbonPageCategoryHeaderControl.Category>
                    <dxr:RibbonPageCategory Caption="Origin"/>
                </dxr:RibbonPageCategoryHeaderControl.Category>
            </dxr:RibbonPageCategoryHeaderControl>
            <dxri:RibbonPagesScrollViewer x:Name="PART_ScrollViewer"
                                          BorderThickness="0"
                                          Focusable="False"
                                          Grid.ColumnSpan="3"
                                          Grid.RowSpan="3"
                                          HorizontalScrollBarVisibility="Disabled"
                                          IsTabStop="False"
                                          Padding="0,0,0,-3"
                                          VerticalContentAlignment="Stretch"
                                          VerticalScrollBarVisibility="Disabled">
                <dxcn:SelectionIndicatorDecorator Style="{DynamicResource {dxrt:RibbonThemeKey ResourceKey=SelectionIndicatorStyle}}">
                    <ItemsPresenter Name="PART_ItemsPresenter" Margin="0,0,0,3"/>
                </dxcn:SelectionIndicatorDecorator>
            </dxri:RibbonPagesScrollViewer>
            <dxr:RibbonPageHeaderControl x:Name="PART_OriginPageHeaderControl"
                                         Grid.ColumnSpan="3"
                                         Grid.Row="2"
                                         Visibility="Hidden"
                                         Width="0">
                <dxr:RibbonPageHeaderControl.Page>
                    <dxr:RibbonPage Caption="Wg"/>
                </dxr:RibbonPageHeaderControl.Page>
            </dxr:RibbonPageHeaderControl>
            <RepeatButton x:Name="PART_LeftRepeatButton"
                          dxcn:WindowChrome.IsHitTestVisibleInChrome="True"
                          Grid.Column="0"
                          Grid.Row="2"
                          Style="{DynamicResource {dxrt:RibbonThemeKey ResourceKey=LeftRepeatButtonStyle}}"
                          Visibility="Hidden"/>
            <RepeatButton x:Name="PART_RightRepeatButton"
                          dxcn:WindowChrome.IsHitTestVisibleInChrome="True"
                          Grid.Column="2"
                          Grid.Row="2"
                          Style="{DynamicResource {dxrt:RibbonThemeKey ResourceKey=RightRepeatButtonStyle}}"
                          Visibility="Hidden"/>
        </Grid>
        <ControlTemplate.Triggers>
            <DataTrigger Binding="{Binding Ribbon.IsRibbonTitleBarActualVisible, RelativeSource={RelativeSource Self}}" Value="False">
                <Setter Property="Visibility"
                        TargetName="PART_OriginHeaderControl"
                        Value="Collapsed"/>
            </DataTrigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxrt:RibbonThemeKey ResourceKey=ControlTemplate}" TargetType="{x:Type dxr:RibbonControl}">
        <dxri:RibbonAutoHideControl Name="PART_AutoHideControl"
                                    Focusable="False"
                                    IsAutoHideActive="{TemplateBinding AutoHideMode}"
                                    IsRibbonShown="{Binding Path=IsHiddenRibbonCollapsed, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay, Converter={StaticResource inverseConverter}}"
                                    IsTabStop="False"
                                    VerticalAlignment="Top">
            <Grid x:Name="PART_DefaultRibbon">
                <ContentControl x:Name="PART_Background"
                                Focusable="False"
                                IsTabStop="False"
                                Template="{DynamicResource {dxrt:RibbonThemeKey ResourceKey=BackgroundTemplate}}"/>
                <Grid x:Name="PART_MainLayout">
                    <Grid.RowDefinitions>
                        <RowDefinition Name="headerAndTabRow" Height="Auto"/>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="Auto"/>
                    </Grid.RowDefinitions>
                    <Grid x:Name="PART_SelectedPage" Grid.Row="1">
                        <ContentControl x:Name="PART_CollapsedSelectedPageBorder" Template="{DynamicResource {dxrt:RibbonThemeKey ResourceKey=CollapsedSelectedPageBorderTemplate}}"/>
                        <dxr:RibbonSelectedPageContentPresenter x:Name="PART_SelectedPageControlContainer"
                                                                HorizontalAlignment="Stretch"
                                                                HorizontalContentAlignment="Stretch"/>
                    </Grid>
                    <Grid x:Name="PART_HeaderAndTabsLayout" UseLayoutRounding="True">
                        <Grid.RowDefinitions>
                            <RowDefinition Name="HeaderRow" Height="Auto"/>
                            <RowDefinition Height="*"/>
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Name="AppBtnColumn" Width="Auto"/>
                            <ColumnDefinition Width="Auto"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Name="lastColumn" Width="Auto"/>
                        </Grid.ColumnDefinitions>
                        <dxr:RibbonPageCategoryHeaderControl x:Name="PART_OriginCategoryHeaderControl"
                                                             Grid.Row="0"
                                                             Visibility="Hidden"
                                                             Width="0">
                            <dxr:RibbonPageCategoryHeaderControl.Category>
                                <dxr:RibbonPageCategory Caption="Wg"/>
                            </dxr:RibbonPageCategoryHeaderControl.Category>
                        </dxr:RibbonPageCategoryHeaderControl>
                        <dxr:RibbonPageHeaderControl x:Name="PART_OriginPageHeaderControl"
                                                     Grid.Row="1"
                                                     Visibility="Hidden"
                                                     Width="0">
                            <dxr:RibbonPageHeaderControl.Page>
                                <dxr:RibbonPage Caption="Wg"/>
                            </dxr:RibbonPageHeaderControl.Page>
                        </dxr:RibbonPageHeaderControl>
                        <ContentControl Name="PART_TabBackground"
                                        Focusable="False"
                                        Grid.ColumnSpan="4"
                                        Grid.Row="0"
                                        Grid.RowSpan="2"
                                        IsTabStop="False"
                                        Template="{DynamicResource {dxrt:RibbonThemeKey ResourceKey=TabPartBorderTemplate}}"/>
                        <Grid x:Name="PART_HeaderBorder"
                              Background="Transparent"
                              Grid.ColumnSpan="4"
                              Grid.Row="0"
                              Visibility="Collapsed"
                              cs:id="Grid_0000">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="Auto"/>
                            </Grid.ColumnDefinitions>
                            <ContentControl Name="HeaderBorderControl"
                                            Background="Transparent"
                                            Focusable="False"
                                            Grid.ColumnSpan="3"
                                            IsTabStop="False"
                                            Template="{TemplateBinding ActualHeaderBorderTemplate}"
                                            cs:id="ContentControl_0000"/>
                            <dxri:RibbonControlBoxContainer x:Name="PART_ControlBoxContainer"
                                                            Grid.Column="2"
                                                            ShowCustomItems="False"
                                                            Visibility="Hidden"/>
                        </Grid>
                        <dxr:RibbonPageCategoriesPane x:Name="PART_PageCategoriesPane"
                                                      Grid.Column="1"
                                                      Grid.ColumnSpan="2"
                                                      Grid.Row="0"
                                                      Grid.RowSpan="2"
                                                      ItemsSource="{TemplateBinding ActualCategories}"/>
                        <dxr:DXRibbonWindowTitlePanel x:Name="PART_TitlePanel"
                                                      ApplicationButton="{Binding ElementName=PART_ApplicationButton}"
                                                      CaptionContentControl="{Binding ElementName=PART_CaptionContainer}"
                                                      Grid.Column="2"
                                                      Grid.ColumnSpan="2"
                                                      Grid.Row="0"
                                                      MinCaptionWidthBlock="{Binding ElementName=PART_MinCaptionBlock}"
                                                      QuickAccessToolbarContainer="{Binding ElementName=PART_ToolbarContainer}"
                                                      RibbonStyle="{TemplateBinding RibbonStyle}"
                                                      Visibility="Collapsed">
                            <ContentControl x:Name="PART_CaptionContainer"
                                            Focusable="False"
                                            HorizontalAlignment="Left"
                                            IsHitTestVisible="False"
                                            IsTabStop="False"
                                            Style="{DynamicResource {dxrt:RibbonThemeKey ResourceKey=CaptionStyle}}">
                                <TextBlock Text="{TemplateBinding ActualWindowTitle}"
                                           TextAlignment="Left"
                                           TextTrimming="CharacterEllipsis"/>
                            </ContentControl>
                            <TextBlock x:Name="PART_MinCaptionBlock"
                                       Text="{Binding ActualWindowTitle, RelativeSource={RelativeSource TemplatedParent}, Converter={dxr:TitleToMinTextWidthConverter}}"
                                       TextAlignment="Left"/>
                        </dxr:DXRibbonWindowTitlePanel>
                        <dxr:RibbonApplicationButtonControl x:Name="PART_ApplicationButton"
                                                            Focusable="False"
                                                            FocusVisualStyle="{x:Null}"
                                                            Grid.Column="0"
                                                            Grid.Row="0"
                                                            Grid.RowSpan="2"
                                                            HorizontalAlignment="Right"
                                                            IsTabStop="False"
                                                            RibbonStyle="{TemplateBinding RibbonStyle}"
                                                            Style="{TemplateBinding ActualApplicationButtonStyle}"/>
                        <ContentControl x:Name="PART_ApplicationIconContainer"
                                        Focusable="False"
                                        Grid.Column="0"
                                        IsTabStop="False"
                                        Style="{DynamicResource {dxrt:RibbonThemeKey ResourceKey=ApplicationIconContainerStyle}}"
                                        Visibility="Collapsed">
                            <dxc:PixelSnapper>
                                <Image MaxHeight="16"
                                       MaxWidth="16"
                                       Source="{TemplateBinding ActualApplicationIcon}"
                                       Stretch="Uniform"/>
                            </dxc:PixelSnapper>
                        </ContentControl>
                        <ContentControl x:Name="PART_ToolbarContainer"
                                        Background="Transparent"
                                        Focusable="False"
                                        Grid.Column="1"
                                        Grid.ColumnSpan="4"
                                        Grid.Row="0"
                                        HorizontalAlignment="Left"
                                        IsTabStop="False"
                                        Style="{TemplateBinding ActualHeaderQuickAccessToolbarContainerStyle}"
                                        VerticalContentAlignment="Center"
                                        cs:id="ContentControl_0001"/>
                        <DockPanel x:Name="PART_RightTabContainer"
                                   dxcn:WindowChrome.IsHitTestVisibleInChrome="True"
                                   Grid.Column="3"
                                   Grid.Row="1"
                                   HorizontalAlignment="Right"
                                   LastChildFill="True"
                                   VerticalAlignment="Bottom">
                            <dxri:RibbonMinimizationButton x:Name="PART_MinimizationButton"
                                                           DockPanel.Dock="Left"
                                                           Style="{DynamicResource {dxrt:RibbonThemeKey ResourceKey=MinimizationButtonStyle}}"
                                                           Template="{DynamicResource {dxrt:RibbonThemeKey ResourceKey=MinimizationButtonTemplate}}"
                                                           Visibility="Collapsed"/>
                            <dxc:DXContentPresenter x:Name="PART_PageHeaderLinksControlContainer"
                                                    DockPanel.Dock="Right"
                                                    VerticalAlignment="Center"/>
                        </DockPanel>
                    </Grid>
                    <ContentControl x:Name="PART_FooterToolbarContainer"
                                    Focusable="False"
                                    Grid.Row="2"
                                    HorizontalAlignment="Stretch"
                                    IsTabStop="False"
                                    Style="{DynamicResource {dxrt:RibbonThemeKey ResourceKey=FooterQuickAccessToolbarContainerStyle}}"
                                    Template="{DynamicResource {dxrt:RibbonThemeKey ResourceKey=FooterQuickAccessToolbarContainerTemplate}}"/>
                </Grid>
            </Grid>
        </dxri:RibbonAutoHideControl>
        <ControlTemplate.Triggers>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="AutoHideMode" Value="True"/>
                    <Condition Property="IsInRibbonWindow" Value="True"/>
                </MultiTrigger.Conditions>
                <Setter Property="Visibility"
                        TargetName="PART_ControlBoxContainer"
                        Value="Visible"/>
                <Setter Property="MinWidth"
                        TargetName="lastColumn"
                        Value="{Binding ElementName=PART_ControlBoxContainer, Path=ActualWidth}"/>
            </MultiTrigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="RibbonStyle" Value="Office2007"/>
                    <Condition Property="IsInRibbonWindow" Value="False"/>
                </MultiTrigger.Conditions>
                <Setter Property="Grid.ColumnSpan"
                        TargetName="PART_ToolbarContainer"
                        Value="1"/>
            </MultiTrigger>
            <Trigger Property="RibbonStyle" Value="Office2010">
                <Setter Property="Grid.Row"
                        TargetName="PART_ApplicationButton"
                        Value="1"/>
                <Setter Property="Grid.RowSpan"
                        TargetName="PART_ApplicationButton"
                        Value="1"/>
                <Setter Property="Grid.Column"
                        TargetName="PART_ToolbarContainer"
                        Value="0"/>
                <Setter Property="Grid.ColumnSpan"
                        TargetName="PART_ToolbarContainer"
                        Value="4"/>
                <Setter Property="Grid.Column"
                        TargetName="PART_PageCategoriesPane"
                        Value="2"/>
                <Setter Property="Grid.ColumnSpan"
                        TargetName="PART_PageCategoriesPane"
                        Value="1"/>
            </Trigger>
            <Trigger Property="RibbonStyle" Value="Office2019">
                <Setter Property="Grid.Row"
                        TargetName="PART_ApplicationButton"
                        Value="1"/>
                <Setter Property="Grid.RowSpan"
                        TargetName="PART_ApplicationButton"
                        Value="1"/>
                <Setter Property="Grid.Column"
                        TargetName="PART_ToolbarContainer"
                        Value="0"/>
                <Setter Property="Grid.ColumnSpan"
                        TargetName="PART_ToolbarContainer"
                        Value="4"/>
                <Setter Property="Grid.Column"
                        TargetName="PART_PageCategoriesPane"
                        Value="2"/>
                <Setter Property="Grid.ColumnSpan"
                        TargetName="PART_PageCategoriesPane"
                        Value="1"/>
            </Trigger>
            <Trigger Property="RibbonStyle" Value="TabletOffice">
                <Setter Property="Grid.Column"
                        TargetName="PART_TitlePanel"
                        Value="1"/>
                <Setter Property="Grid.ColumnSpan"
                        TargetName="PART_TitlePanel"
                        Value="3"/>
                <Setter Property="Grid.Row"
                        TargetName="PART_ApplicationButton"
                        Value="1"/>
                <Setter Property="Grid.RowSpan"
                        TargetName="PART_ApplicationButton"
                        Value="1"/>
                <Setter Property="Grid.Row"
                        TargetName="PART_ToolbarContainer"
                        Value="1"/>
                <Setter Property="Grid.Column"
                        TargetName="PART_ToolbarContainer"
                        Value="1"/>
                <Setter Property="Grid.ColumnSpan"
                        TargetName="PART_ToolbarContainer"
                        Value="1"/>
                <Setter Property="HorizontalAlignment"
                        TargetName="PART_PageCategoriesPane"
                        Value="Center"/>
                <Setter Property="Grid.Column"
                        TargetName="PART_PageCategoriesPane"
                        Value="2"/>
                <Setter Property="Grid.ColumnSpan"
                        TargetName="PART_PageCategoriesPane"
                        Value="1"/>
            </Trigger>
            <Trigger Property="RibbonStyle" Value="OfficeSlim">
                <Setter Property="Grid.Row"
                        TargetName="PART_ApplicationButton"
                        Value="1"/>
                <Setter Property="Grid.RowSpan"
                        TargetName="PART_ApplicationButton"
                        Value="1"/>
                <Setter Property="Grid.Column"
                        TargetName="PART_ToolbarContainer"
                        Value="0"/>
                <Setter Property="Grid.ColumnSpan"
                        TargetName="PART_ToolbarContainer"
                        Value="4"/>
                <Setter Property="ItemsPanel"
                        TargetName="PART_PageCategoriesPane"
                        Value="{DynamicResource {dxrt:RibbonPageCategoriesPaneThemeKey ResourceKey=OfficeSlimPanelTemplate}}"/>
            </Trigger>
            <Trigger Property="ShowApplicationButton" Value="False">
                <Setter Property="Visibility"
                        TargetName="PART_ApplicationButton"
                        Value="Collapsed"/>
                <Setter Property="Grid.ColumnSpan"
                        TargetName="PART_ApplicationIconContainer"
                        Value="2"/>
                <Setter Property="MaxWidth"
                        TargetName="AppBtnColumn"
                        Value="0"/>
            </Trigger>
            <Trigger Property="RibbonHeaderVisibility" Value="Collapsed">
                <Setter Property="Height"
                        TargetName="headerAndTabRow"
                        Value="0"/>
            </Trigger>
            <Trigger Property="IsInRibbonWindow" Value="True">
                <Setter Property="Visibility"
                        TargetName="PART_TitlePanel"
                        Value="Visible"/>
                <Setter Property="Visibility"
                        TargetName="PART_HeaderBorder"
                        Value="Visible"/>
                <Setter Property="MinHeight"
                        TargetName="HeaderRow"
                        Value="{DynamicResource {dxrt:RibbonThemeKey ResourceKey=HeaderMinHeight}}"/>
                <Setter Property="MinHeight"
                        TargetName="PART_ToolbarContainer"
                        Value="{DynamicResource {dxrt:RibbonThemeKey ResourceKey=HeaderMinHeight}}"/>
            </Trigger>
            <DataTrigger Binding="{Binding Path=(dxcn:DXWindowsHelper.Window).(dxc:ThemedWindow.ShowTitle), RelativeSource={RelativeSource Self}}" Value="False">
                <Setter Property="Visibility"
                        TargetName="PART_TitlePanel"
                        Value="Collapsed"/>
            </DataTrigger>
            <MultiDataTrigger>
                <MultiDataTrigger.Conditions>
                    <Condition Binding="{Binding Path=IsInRibbonWindow, RelativeSource={RelativeSource Self}}" Value="True"/>
                    <Condition Binding="{Binding Path=(dxcn:DXWindowsHelper.WindowKind), RelativeSource={RelativeSource Self}}" Value="{x:Static dxcn:DXWindowsHelper.ThemedWindow}"/>
                    <Condition Binding="{Binding Path=(dxcn:DXWindowsHelper.Window).(dxc:ThemedWindow.ActualWindowKind), RelativeSource={RelativeSource Self}}" Value="Ribbon"/>
                </MultiDataTrigger.Conditions>
                <Setter Property="MinHeight"
                        TargetName="HeaderRow"
                        Value="{Binding Path=(dxcn:WindowChrome.CaptionHeight), RelativeSource={RelativeSource AncestorType={x:Type dxc:ThemedWindow}}}"/>
            </MultiDataTrigger>
            <Trigger Property="IsApplicationIconVisible" Value="True">
                <Setter Property="Visibility"
                        TargetName="PART_ApplicationIconContainer"
                        Value="Visible"/>
            </Trigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="MinimizationButtonPosition" Value="PageHeader"/>
                    <Condition Property="IsMinimizationButtonVisible" Value="True"/>
                </MultiTrigger.Conditions>
                <Setter Property="Visibility"
                        TargetName="PART_MinimizationButton"
                        Value="Visible"/>
            </MultiTrigger>
            <MultiTrigger>
                <MultiTrigger.Conditions>
                    <Condition Property="IsRibbonTitleBarActualVisible" Value="False"/>
                    <Condition Property="IsInRibbonWindow" Value="False"/>
                </MultiTrigger.Conditions>
                <Setter Property="Visibility"
                        TargetName="PART_OriginCategoryHeaderControl"
                        Value="Collapsed"/>
            </MultiTrigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    <ControlTemplate x:Key="{dxrt:RibbonThemeKey ResourceKey=AeroTemplate}" TargetType="{x:Type dxr:RibbonControl}">
        <Grid x:Name="PART_DefaultRibbon" VerticalAlignment="Top">
            <ContentControl x:Name="PART_Background"
                            Focusable="False"
                            IsTabStop="False"
                            Template="{DynamicResource {dxrt:RibbonThemeKey ResourceKey=BackgroundTemplateInRibbonAeroWindow}}"/>
            <Grid x:Name="PART_MainLayout">
                <Grid.RowDefinitions>
                    <RowDefinition Name="headerAndTabRow" Height="Auto"/>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="Auto"/>
                </Grid.RowDefinitions>
                <Grid x:Name="PART_SelectedPage" Grid.Row="1">
                    <ContentControl x:Name="PART_CollapsedSelectedPageBorder" Template="{DynamicResource {dxrt:RibbonThemeKey ResourceKey=CollapsedSelectedPageBorderTemplateInAeroWindow}}"/>
                    <dxr:RibbonSelectedPageContentPresenter x:Name="PART_SelectedPageControlContainer"/>
                </Grid>
                <Grid x:Name="PART_HeaderAndTabsLayout" Grid.IsSharedSizeScope="True">
                    <Grid.RowDefinitions>
                        <RowDefinition Name="HeaderRow" Height="Auto"/>
                        <RowDefinition Height="*"/>
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="Auto"/>
                    </Grid.ColumnDefinitions>
                    <ContentControl Grid.ColumnSpan="4"
                                    Grid.RowSpan="2"
                                    Template="{DynamicResource {dxrt:RibbonThemeKey ResourceKey=HeaderAndTabsBorderTemplatedInAeroWindow}}"/>
                    <Grid x:Name="PART_HeaderBorder"
                          Background="Transparent"
                          Grid.ColumnSpan="4"
                          Grid.Row="0"
                          cs:id="Grid_0000">
                        <ContentControl Focusable="False"
                                        IsTabStop="False"
                                        Template="{DynamicResource {dxrt:RibbonThemeKey ResourceKey=HeaderBorderTemplateInRibbonAeroWindow}}"
                                        Visibility="{Binding Path=IsInRibbonWindow, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource boolToVisibilityConverter}}"/>
                        <ContentControl Focusable="False"
                                        IsTabStop="False"
                                        Template="{TemplateBinding ActualHeaderBorderTemplate}"
                                        Visibility="{Binding Path=IsInRibbonWindow, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource invertBoolToVisibilityConverter}}"/>
                    </Grid>
                    <dxr:DXRibbonWindowTitlePanel x:Name="PART_TitlePanel"
                                                  CaptionContentControl="{Binding ElementName=PART_CaptionContainer}"
                                                  Grid.Column="1"
                                                  Grid.ColumnSpan="3"
                                                  Grid.Row="0"
                                                  MinCaptionWidthBlock="{Binding ElementName=PART_MinCaptionBlock}"
                                                  QuickAccessToolbarContainer="{Binding ElementName=PART_ToolbarContainer}"
                                                  RibbonStyle="{TemplateBinding RibbonStyle}"
                                                  Visibility="Collapsed">
                        <ContentControl x:Name="PART_CaptionContainer"
                                        Content="{TemplateBinding ActualWindowTitle}"
                                        Focusable="False"
                                        Grid.Column="1"
                                        HorizontalAlignment="Left"
                                        IsHitTestVisible="False"
                                        IsTabStop="False"
                                        Style="{DynamicResource {dxrt:RibbonThemeKey ResourceKey=CaptionStyleInRibbonAeroWindow}}"/>
                        <TextBlock x:Name="PART_MinCaptionBlock"
                                   Text="{Binding ActualWindowTitle, RelativeSource={RelativeSource TemplatedParent}, Converter={dxr:TitleToMinTextWidthConverter}}"
                                   TextAlignment="Left"/>
                    </dxr:DXRibbonWindowTitlePanel>
                    <ContentControl Name="PART_TabBackground"
                                    Focusable="False"
                                    Grid.ColumnSpan="4"
                                    Grid.Row="1"
                                    IsTabStop="False"
                                    Template="{DynamicResource {dxrt:RibbonThemeKey ResourceKey=TabPartBorderTemplateInRibbonAeroWindow}}"/>
                    <dxr:RibbonApplicationButtonControl x:Name="PART_ApplicationButton"
                                                        Focusable="False"
                                                        FocusVisualStyle="{x:Null}"
                                                        Grid.RowSpan="2"
                                                        IsTabStop="False"
                                                        RibbonStyle="{TemplateBinding RibbonStyle}"
                                                        Style="{TemplateBinding ActualApplicationButtonStyle}"/>
                    <dxr:RibbonPageCategoriesPane x:Name="PART_PageCategoriesPane"
                                                  Grid.Column="1"
                                                  Grid.ColumnSpan="2"
                                                  Grid.Row="0"
                                                  Grid.RowSpan="2"
                                                  ItemsSource="{TemplateBinding ActualCategories}"/>
                    <ContentControl x:Name="PART_ApplicationIconContainer"
                                    Focusable="False"
                                    Grid.Column="0"
                                    IsTabStop="False"
                                    Style="{DynamicResource {dxrt:RibbonThemeKey ResourceKey=ApplicationIconContainerStyleInRibbonAeroWindow}}"
                                    Visibility="Collapsed">
                        <dxc:PixelSnapper>
                            <Image MaxHeight="16"
                                   MaxWidth="16"
                                   Source="{TemplateBinding ActualApplicationIcon}"
                                   Stretch="Uniform"/>
                        </dxc:PixelSnapper>
                    </ContentControl>
                    <ContentControl x:Name="PART_ToolbarContainer"
                                    Background="Transparent"
                                    Focusable="False"
                                    Grid.Column="1"
                                    Grid.Row="0"
                                    HorizontalAlignment="Left"
                                    IsTabStop="False"
                                    Margin="0"
                                    Style="{TemplateBinding ActualHeaderQuickAccessToolbarContainerStyle}"
                                    VerticalContentAlignment="Center"
                                    cs:id="ContentControl_0000"/>
                    <DockPanel x:Name="PART_RightTabContainer"
                               dxcn:WindowChrome.IsHitTestVisibleInChrome="True"
                               Grid.Column="3"
                               Grid.Row="1"
                               HorizontalAlignment="Right"
                               LastChildFill="True"
                               VerticalAlignment="Bottom">
                        <dxri:RibbonMinimizationButton x:Name="PART_MinimizationButton"
                                                       DockPanel.Dock="Left"
                                                       Style="{DynamicResource {dxrt:RibbonThemeKey ResourceKey=MinimizationButtonStyleInRibbonAeroWindow}}"
                                                       Template="{DynamicResource {dxrt:RibbonThemeKey ResourceKey=MinimizationButtonTemplateInRibbonAeroWindow}}"
                                                       Visibility="Collapsed"/>
                        <dxc:DXContentPresenter x:Name="PART_PageHeaderLinksControlContainer"
                                                DockPanel.Dock="Right"
                                                VerticalAlignment="Stretch"
                                                VerticalContentAlignment="Center"/>
                    </DockPanel>
                </Grid>
                <ContentControl x:Name="PART_FooterToolbarContainer"
                                Focusable="False"
                                Grid.Row="2"
                                HorizontalAlignment="Stretch"
                                IsTabStop="False"
