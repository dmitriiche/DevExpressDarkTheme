    <Style x:Key="{dxscht:SchedulerButtonThemeKey ResourceKey=NavigationButton_BackwardStyle}"
           BasedOn="{StaticResource NavigationButton.BaseStyle}"
           TargetType="dxschv:SchedulerButton">
        <Setter Property="Margin" Value="{StaticResource NavigationButton.Backward.Margin}"/>
        <Setter Property="CornerRadius" Value="{StaticResource NavigationButton.Backward.CornerRadius}"/>
        <Setter Property="HoverCornerRadius" Value="{StaticResource NavigationButton.Backward.CornerRadius}"/>
        <Setter Property="PressedCornerRadius" Value="{StaticResource NavigationButton.Backward.CornerRadius}"/>
        <Setter Property="ContentTemplate">
            <Setter.Value>
                <DataTemplate>
                    <dxschv:NavigationButtonPanel>
                        <Path Fill="{Binding '(TextBlock.Foreground)', RelativeSource={RelativeSource Self}}"
                              Margin="4,12,4,12"
                              Style="{StaticResource UIShape.NavigationButton.Prev}"/>
                        <TextBlock HorizontalAlignment="Center"
                                   Margin="4,0,4,24"
                                   Text="{dxschc:SchedulerLocalizer StringId=Caption_PrevAppointment}"
                                   TextAlignment="Center"
                                   VerticalAlignment="Center"
                                   Width="118">
                            <TextBlock.LayoutTransform>
                                <RotateTransform Angle="-90"/>
                            </TextBlock.LayoutTransform>
                        </TextBlock>
                    </dxschv:NavigationButtonPanel>
                </DataTemplate>
            </Setter.Value>
        </Setter>
    </Style>

