    <Style x:Key="{dxscht:SchedulerButtonThemeKey ResourceKey=MoreButton_UpStyle}"
           BasedOn="{StaticResource MoreButton.BaseStyle}"
           TargetType="dxschv:SchedulerButton">
        <Setter Property="ContentTemplate">
            <Setter.Value>
                <DataTemplate>
                    <Path Fill="{Binding '(TextBlock.Foreground)', RelativeSource={RelativeSource Self}}" Style="{StaticResource UIShape.Up}"/>
                </DataTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="{dxscht:SchedulerButtonThemeKey ResourceKey=MoreButton_DownStyle}"
           BasedOn="{StaticResource MoreButton.BaseStyle}"
           TargetType="dxschv:SchedulerButton">
