    <ResourceDictionary.MergedDictionaries>
        <dxt:ResourceDictionaryEx Source="{dx:SchedulingResource UICore.xaml, ShortPaths=True}"/>
        <dxt:ResourceDictionaryEx Source="{dx:SchedulingResource UIPreset.xaml, ShortPaths=True}"/>
    </ResourceDictionary.MergedDictionaries>

    <Style x:Key="TextButton.Style" TargetType="dxschv:SchedulerButton">
        <Setter Property="Template" Value="{DynamicResource {dxscht:SchedulerCommonThemeKey ResourceKey=ButtonTemplate}}"/>
        <Setter Property="Padding" Value="9,5,9,5"/>
        <Setter Property="HorizontalContentAlignment" Value="Center"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="Foreground" Value="{StaticResource DateNavigationPanelButton.Normal.Foreground}"/>
        <Setter Property="Background" Value="{StaticResource DateNavigationPanelButton.NormalBackground}"/>
        <Setter Property="BorderBrush" Value="{StaticResource DateNavigationPanelButton.NormalBorderBrush}"/>
        <Setter Property="BorderThickness" Value="{StaticResource DateNavigationPanelButton.BorderThickness}"/>
        <Setter Property="CornerRadius" Value="{StaticResource DateNavigationPanelButton.CornerRadius}"/>
        <Setter Property="HoverForeground" Value="{StaticResource DateNavigationPanelButton.Hover.Foreground}"/>
        <Setter Property="HoverBackground" Value="{StaticResource DateNavigationPanelButton.HoverBackground}"/>
        <Setter Property="HoverBorderBrush" Value="{StaticResource DateNavigationPanelButton.HoverBorderBrush}"/>
        <Setter Property="HoverBorderThickness" Value="{StaticResource DateNavigationPanelButton.BorderThickness}"/>
        <Setter Property="HoverCornerRadius" Value="{StaticResource DateNavigationPanelButton.CornerRadius}"/>
        <Setter Property="PressedForeground" Value="{StaticResource DateNavigationPanelButton.Pressed.Foreground}"/>
        <Setter Property="PressedBackground" Value="{StaticResource DateNavigationPanelButton.PressedBackground}"/>
        <Setter Property="PressedBorderBrush" Value="{StaticResource DateNavigationPanelButton.PressedBorderBrush}"/>
        <Setter Property="PressedBorderThickness" Value="{StaticResource DateNavigationPanelButton.BorderThickness}"/>
        <Setter Property="PressedCornerRadius" Value="{StaticResource DateNavigationPanelButton.CornerRadius}"/>
        <Setter Property="DisabledBackground" Value="{StaticResource DateNavigationPanelButton.DisabledBackground}"/>
        <Setter Property="DisabledBorderBrush" Value="{StaticResource DateNavigationPanelButton.DisabledBorderBrush}"/>
    </Style>

    <Style x:Key="MoreButton.BaseStyle" TargetType="dxschv:SchedulerButton">
        <Setter Property="Template" Value="{DynamicResource {dxscht:SchedulerCommonThemeKey ResourceKey=ButtonTemplate}}"/>
        <Setter Property="Padding" Value="0"/>
        <Setter Property="Width" Value="{StaticResource MoreButton.Width}"/>
        <Setter Property="Height" Value="{StaticResource MoreButton.Height}"/>
        <Setter Property="Margin" Value="{StaticResource MoreButton.Margin}"/>
        <Setter Property="Cursor" Value="Hand"/>
        <Setter Property="HorizontalContentAlignment" Value="Stretch"/>
        <Setter Property="VerticalContentAlignment" Value="Stretch"/>
        <Setter Property="dxsch:HitTestBehavior.HitTestType" Value="MoreButton"/>
        <Setter Property="Foreground" Value="{StaticResource MoreButton.Normal.Foreground}"/>
        <Setter Property="Background" Value="{StaticResource MoreButton.NormalBackground}"/>
        <Setter Property="BorderBrush" Value="{StaticResource MoreButton.NormalBorderBrush}"/>
        <Setter Property="BorderThickness" Value="{StaticResource MoreButton.BorderThickness}"/>
        <Setter Property="CornerRadius" Value="{StaticResource MoreButton.CornerRadius}"/>
