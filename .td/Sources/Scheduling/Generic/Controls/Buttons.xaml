        <Setter Property="HoverCornerRadius" Value="{StaticResource MoreButton.CornerRadius}"/>
        <Setter Property="PressedForeground" Value="{StaticResource MoreButton.Pressed.Foreground}"/>
        <Setter Property="PressedBackground" Value="{StaticResource MoreButton.PressedBackground}"/>
        <Setter Property="PressedBorderBrush" Value="{StaticResource MoreButton.PressedBorderBrush}"/>
        <Setter Property="PressedBorderThickness" Value="{StaticResource MoreButton.BorderThickness}"/>
        <Setter Property="PressedCornerRadius" Value="{StaticResource MoreButton.CornerRadius}"/>
        <Setter Property="DisabledBackground" Value="{StaticResource MoreButton.DisabledBackground}"/>
        <Setter Property="DisabledBorderBrush" Value="{StaticResource MoreButton.DisabledBorderBrush}"/>
    </Style>

    <Style x:Key="NavigationButton.BaseStyle" TargetType="dxschv:SchedulerButton">
        <Setter Property="Template" Value="{DynamicResource {dxscht:SchedulerCommonThemeKey ResourceKey=ButtonTemplate}}"/>
        <Setter Property="Padding" Value="0"/>
        <Setter Property="FontSize" Value="{StaticResource NavigationButton.FontSize}"/>
        <Setter Property="Cursor" Value="Hand"/>
        <Setter Property="HorizontalContentAlignment" Value="Stretch"/>
        <Setter Property="VerticalContentAlignment" Value="Stretch"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="dxsch:HitTestBehavior.HitTestType" Value="NavigationButton"/>
        <Setter Property="Width" Value="{StaticResource NavigationButton.Width}"/>
        <Setter Property="Height" Value="{StaticResource NavigationButton.Height}"/>
        <Setter Property="BorderThickness" Value="{StaticResource NavigationButton.BorderThickness}"/>
        <Setter Property="HoverBorderThickness" Value="{StaticResource NavigationButton.BorderThickness}"/>
        <Setter Property="PressedBorderThickness" Value="{StaticResource NavigationButton.BorderThickness}"/>
        <Setter Property="dxmvvm:Interaction.BehaviorsTemplate">
            <Setter.Value>
                <DataTemplate>
                    <ContentControl>
                        <dxschc:SchedulerNavigationButtonColorizationBehavior/>
                    </ContentControl>
                </DataTemplate>
            </Setter.Value>
        </Setter>
    </Style>

