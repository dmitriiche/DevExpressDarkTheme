<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dxdst="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys"
                    xmlns:dxschc="http://schemas.devexpress.com/winfx/2008/xaml/scheduling"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="BrushSet.StatusBusy" Color="#FF4A87E2"/>

    <SolidColorBrush x:Key="BrushSet.StatusFree" Color="White"/>

    <SolidColorBrush x:Key="BrushSet.StatusOutOfOffice" Color="#FFD95353"/>

    <LinearGradientBrush x:Key="BrushSet.StatusTentative"
                         EndPoint="4,4"
                         MappingMode="Absolute"
                         SpreadMethod="Repeat"
                         StartPoint="0,0">
        <GradientStop Color="#FF4A87E2"
                      Offset="0"
                      cs:id="GradientStop_0000"/>
        <GradientStop Color="#FF4A87E2"
                      Offset="0.5"
                      cs:id="GradientStop_0001"/>
        <GradientStop Color="White"
                      Offset="0.5"
                      cs:id="GradientStop_0002"/>
        <GradientStop Color="White"
                      Offset="1"
                      cs:id="GradientStop_0003"/>
    </LinearGradientBrush>

    <DrawingBrush x:Key="BrushSet.StatusWorkingElsewhere"
                  TileMode="Tile"
                  Viewport="0,0,4,4"
                  ViewportUnits="Absolute">
        <DrawingBrush.Drawing>
            <DrawingGroup>
                <GeometryDrawing Brush="White" cs:id="GeometryDrawing_0000">
                    <GeometryDrawing.Geometry>
                        <GeometryGroup FillRule="Nonzero">
                            <RectangleGeometry Rect="0,0,80,80"/>
                        </GeometryGroup>
                    </GeometryDrawing.Geometry>
                </GeometryDrawing>
                <GeometryDrawing Brush="#FF937BD1" cs:id="GeometryDrawing_0001">
                    <GeometryDrawing.Geometry>
                        <GeometryGroup FillRule="Nonzero">
                            <RectangleGeometry Rect="0,0,20,20"/>
                            <RectangleGeometry Rect="40,40,20,20"/>
                        </GeometryGroup>
                    </GeometryDrawing.Geometry>
                </GeometryDrawing>
            </DrawingGroup>
        </DrawingBrush.Drawing>
    </DrawingBrush>

    <dxschc:BrightnessColorTransform x:Key="BrushSet.TimeRegion.Color1.Transform.Light"
                                     Brightness="-0.4"
                                     OverrideA="#59"/>

    <dxschc:BrightnessColorTransform x:Key="BrushSet.TimeRegion.Color2.Transform.Light"
                                     Brightness="0.6"
                                     OverrideA="#40"/>

    <dxschc:BrightnessColorTransform x:Key="BrushSet.TimeRegion.Color1.Transform.Dark"
                                     Brightness="0.6"
                                     OverrideA="#59"/>

    <dxschc:BrightnessColorTransform x:Key="BrushSet.TimeRegion.Color2.Transform.Dark"
                                     Brightness="-0.4"
                                     OverrideA="#40"/>

    <!--ScheduleView-->

    <SolidColorBrush x:Key="ScheuleView.TimeScale.TickBrush" Color="DarkGray"/>

    <SolidColorBrush x:Key="ScheuleView.TimeScale.BorderBrush" Color="Black"/>

</ResourceDictionary>