        </Setter>
    </Style>

    <Style x:Key="{dxdst:DefaultStyleThemeKey FullName=DevExpress.Xpf.Gauges.IStyleLinearScaleLineControl}" TargetType="local:IStyleLinearScaleLineControl">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="local:IStyleLinearScaleLineControl">
                    <Rectangle Fill="{Binding Path=Presentation.ActualFill}"
                               RenderTransformOrigin="0.5, 0"
                               StrokeThickness="0"/>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="{dxdst:DefaultStyleThemeKey FullName=DevExpress.Xpf.Gauges.YellowSubmarineLinearScaleLineControl}" TargetType="local:YellowSubmarineLinearScaleLineControl">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="local:YellowSubmarineLinearScaleLineControl">
                    <Rectangle Fill="{Binding Path=Presentation.ActualFill}"
                               RenderTransformOrigin="0.5, 0"
                               StrokeThickness="0"/>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="{dxdst:DefaultStyleThemeKey FullName=DevExpress.Xpf.Gauges.MagicLightLinearScaleLineControl}" TargetType="local:MagicLightLinearScaleLineControl">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="local:MagicLightLinearScaleLineControl">
                    <Rectangle Fill="{Binding Path=Presentation.ActualFill}"
                               RenderTransformOrigin="0.5, 0"
                               StrokeThickness="0"/>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="{dxdst:DefaultStyleThemeKey FullName=DevExpress.Xpf.Gauges.RedThermometerLinearScaleLineControl}" TargetType="local:RedThermometerLinearScaleLineControl">
        <Setter Property="Template">
            <Setter.Value>
