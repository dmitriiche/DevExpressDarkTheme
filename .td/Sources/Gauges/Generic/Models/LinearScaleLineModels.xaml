<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:core="http://schemas.devexpress.com/winfx/2008/xaml/core"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dxdst="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys"
                    xmlns:dxgt="http://schemas.devexpress.com/winfx/2008/xaml/gauges/themekeys"
                    xmlns:local="http://schemas.devexpress.com/winfx/2008/xaml/gauges"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Style x:Key="{dxdst:DefaultStyleThemeKey FullName=DevExpress.Xpf.Gauges.DefaultLinearScaleLineControl}" TargetType="local:DefaultLinearScaleLineControl">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="local:DefaultLinearScaleLineControl">
                    <Rectangle Fill="{Binding Path=Presentation.ActualFill}" RenderTransformOrigin="0.5, 0"/>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="{dxdst:DefaultStyleThemeKey FullName=DevExpress.Xpf.Gauges.CleanWhiteLinearScaleLineControl}" TargetType="local:CleanWhiteLinearScaleLineControl">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="local:CleanWhiteLinearScaleLineControl">
                    <Rectangle Fill="{Binding Path=Presentation.ActualFill}" RenderTransformOrigin="0.5, 0"/>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="{dxdst:DefaultStyleThemeKey FullName=DevExpress.Xpf.Gauges.CosmicLinearScaleLineControl}" TargetType="local:CosmicLinearScaleLineControl">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="local:CosmicLinearScaleLineControl">
                    <Rectangle Fill="{Binding Path=Presentation.ActualFill}" RenderTransformOrigin="0.5, 0"/>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="{dxdst:DefaultStyleThemeKey FullName=DevExpress.Xpf.Gauges.SmartLinearScaleLineControl}" TargetType="local:SmartLinearScaleLineControl">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="local:SmartLinearScaleLineControl">
                    <Rectangle Fill="{Binding Path=Presentation.ActualFill}" RenderTransformOrigin="0.5, 0"/>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="{dxdst:DefaultStyleThemeKey FullName=DevExpress.Xpf.Gauges.ProgressiveLinearScaleLineControl}" TargetType="local:ProgressiveLinearScaleLineControl">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="local:ProgressiveLinearScaleLineControl">
                    <Rectangle Fill="{Binding Path=Presentation.ActualFill}"
                               RenderTransformOrigin="0.5, 0"
                               StrokeThickness="0"/>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="{dxdst:DefaultStyleThemeKey FullName=DevExpress.Xpf.Gauges.EcoLinearScaleLineControl}" TargetType="local:EcoLinearScaleLineControl">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="local:EcoLinearScaleLineControl">
                    <Rectangle Fill="{Binding Path=Presentation.ActualFill}"
                               RenderTransformOrigin="0.5, 0"
                               StrokeThickness="0"/>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="{dxdst:DefaultStyleThemeKey FullName=DevExpress.Xpf.Gauges.FutureLinearScaleLineControl}" TargetType="local:FutureLinearScaleLineControl">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="local:FutureLinearScaleLineControl">
                    <Rectangle Fill="{Binding Path=Presentation.ActualFill}"
                               RenderTransformOrigin="0.5, 0"
                               StrokeThickness="0"/>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="{dxdst:DefaultStyleThemeKey FullName=DevExpress.Xpf.Gauges.ClassicLinearScaleLineControl}" TargetType="local:ClassicLinearScaleLineControl">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="local:ClassicLinearScaleLineControl">
                    <Rectangle Fill="{Binding Path=Presentation.ActualFill}"
                               RenderTransformOrigin="0.5, 0"
                               StrokeThickness="0"/>
                </ControlTemplate>
