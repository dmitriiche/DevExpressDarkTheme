                               StrokeThickness="0"/>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="{dxdst:DefaultStyleThemeKey FullName=DevExpress.Xpf.Gauges.FlatLightLinearScaleLineControl}" TargetType="local:FlatLightLinearScaleLineControl">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="local:FlatLightLinearScaleLineControl">
                    <Rectangle Fill="{Binding Path=Presentation.ActualFill}"
                               Margin="0, -1, 0, -1"
                               RenderTransformOrigin="0.5, 0"/>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="{dxdst:DefaultStyleThemeKey FullName=DevExpress.Xpf.Gauges.FlatDarkLinearScaleLineControl}" TargetType="local:FlatDarkLinearScaleLineControl">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="local:FlatDarkLinearScaleLineControl">
                    <Rectangle Fill="{Binding Path=Presentation.ActualFill}"
                               Margin="0, -1, 0, -1"
                               RenderTransformOrigin="0.5, 0"/>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="{dxdst:DefaultStyleThemeKey FullName=DevExpress.Xpf.Gauges.ThemeableLinearScaleLineControl}" TargetType="local:ThemeableLinearScaleLineControl">
        <Setter Property="ThemeFill" Value="{DynamicResource {dxgt:GaugesBrushesThemeKey ResourceKey=ScaleElementsBrush}}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="local:ThemeableLinearScaleLineControl">
                    <Rectangle Fill="{Binding Path=ActualFill, RelativeSource={RelativeSource TemplatedParent}}"
                               Margin="0, -1, 0, -1"
                               RenderTransformOrigin="0.5, 0"/>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

</ResourceDictionary>