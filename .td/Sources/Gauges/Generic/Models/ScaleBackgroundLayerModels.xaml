    <Style x:Key="{dxdst:DefaultStyleThemeKey FullName=DevExpress.Xpf.Gauges.DefaultArcScaleBackgroundLayerControl}" TargetType="local:DefaultArcScaleBackgroundLayerControl">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="local:DefaultArcScaleBackgroundLayerControl">
                    <Grid Margin="0" RenderTransformOrigin="0.5, 0.5">
                        <Ellipse Fill="#FFB0B0B0"
                                 Margin="0"
                                 cs:id="Ellipse_0000"/>
                        <Ellipse Fill="#FFEEEEEE"
                                 Margin="1"
                                 StrokeThickness="0"
                                 cs:id="Ellipse_0001"/>
                        <Ellipse Fill="#FFB0B0B0"
                                 Margin="12"
                                 StrokeThickness="0"
                                 cs:id="Ellipse_0002"/>
                        <Ellipse Fill="{Binding Path=Presentation.ActualFill}"
                                 Margin="13"
                                 StrokeThickness="0"/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

