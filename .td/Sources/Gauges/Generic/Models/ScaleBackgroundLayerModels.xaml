                                        <RadialGradientBrush Center="0.5,1"
                                                             GradientOrigin="0.5,1"
                                                             RadiusX="1"
                                                             RadiusY="1"
                                                             cs:id="RadialGradientBrush_0002">
                                            <RadialGradientBrush.RelativeTransform>
                                                <RotateTransform Angle="-45"
                                                                 CenterX="0.5"
                                                                 CenterY="1"/>
                                            </RadialGradientBrush.RelativeTransform>
                                            <GradientStop Color="#FFCDCFD6"
                                                          Offset="1"
                                                          cs:id="GradientStop_0007"/>
                                            <GradientStop Color="#FFF4F5F6" cs:id="GradientStop_0008"/>
                                        </RadialGradientBrush>
                                    </Ellipse.Fill>
                                </Ellipse>
                                <Ellipse Margin="1" StrokeThickness="0">
                                    <Ellipse.Fill>
                                        <RadialGradientBrush Center="1.206,0.293"
                                                             GradientOrigin="1.206,0.293"
                                                             RadiusX="1"
