    <Style x:Key="{dxdst:DefaultStyleThemeKey FullName=DevExpress.Xpf.Gauges.CleanWhiteArcScaleBackgroundLayerControl}" TargetType="local:CleanWhiteArcScaleBackgroundLayerControl">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="local:CleanWhiteArcScaleBackgroundLayerControl">
                    <Grid RenderTransformOrigin="0.5, 0.5">
                        <Ellipse Margin="2,4,2,0" Opacity="0.15">
                            <Ellipse.Fill>
                                <RadialGradientBrush cs:id="RadialGradientBrush_0000">
                                    <GradientStop Color="Black"
                                                  Offset="0"
                                                  cs:id="GradientStop_0000"/>
                                    <GradientStop Color="#00000000"
                                                  Offset="1"
                                                  cs:id="GradientStop_0001"/>
                                    <GradientStop Color="Black"
                                                  Offset="0.96"
                                                  cs:id="GradientStop_0002"/>
                                </RadialGradientBrush>
                            </Ellipse.Fill>
                            <Ellipse.OpacityMask>
                                <LinearGradientBrush EndPoint="0.5,1"
                                                     StartPoint="0.5,0"
                                                     cs:id="LinearGradientBrush_0000">
                                    <GradientStop Color="Black"
                                                  Offset="0"
                                                  cs:id="GradientStop_0003"/>
                                    <GradientStop Color="White"
                                                  Offset="1"
                                                  cs:id="GradientStop_0004"/>
                                </LinearGradientBrush>
                            </Ellipse.OpacityMask>
                        </Ellipse>
                        <Grid Margin="3">
                            <Grid>
                                <Ellipse Margin="0">
                                    <Ellipse.Fill>
                                        <RadialGradientBrush Center="0.5,1"
                                                             GradientOrigin="0.5,1"
