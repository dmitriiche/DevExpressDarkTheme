    <Style x:Key="{dxdst:DefaultStyleThemeKey FullName=DevExpress.Xpf.Gauges.ProgressiveArcScaleLineControl}" TargetType="local:ProgressiveArcScaleLineControl">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="local:ProgressiveArcScaleLineControl">
                    <Ellipse Fill="{Binding Path=Presentation.ActualFill}"
                             RenderTransformOrigin="0.5, 0.5"
                             StrokeThickness="0"/>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

