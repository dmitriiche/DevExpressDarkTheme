    </Style>

    <Style x:Key="{dxdst:DefaultStyleThemeKey FullName=DevExpress.Xpf.Gauges.SmartArcScaleLineControl}" TargetType="local:SmartArcScaleLineControl">
        <Setter Property="Template">
            <Setter.Value>
