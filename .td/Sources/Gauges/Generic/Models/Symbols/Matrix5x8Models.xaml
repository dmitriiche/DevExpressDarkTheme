    <Style x:Key="{dxdst:DefaultStyleThemeKey FullName=DevExpress.Xpf.Gauges.DefaultMatrix5x8Control}"
           BasedOn="{StaticResource SymbolStyleBase}"
           TargetType="local:DefaultMatrix5x8Control">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="local:DefaultMatrix5x8Control">
                    <Viewbox>
                        <local:MatrixSymbolRenderControl Height="118"
                                                         HorizontalSegmentCount="5"
                                                         HorizontalSegmentSpacing="2"
                                                         Presentation="{Binding Presentation}"
                                                         SegmentHeight="12"
                                                         SegmentsLeft="4"
                                                         SegmentsTop="4"
                                                         SegmentWidth="12"
                                                         SnapsToDevicePixels="False"
                                                         SymbolState="{Binding SymbolState}"
                                                         VerticalSegmentCount="8"
                                                         VerticalSegmentSpacing="2"
                                                         Width="76"/>
                    </Viewbox>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

