<ResourceDictionary mc:Ignorable="cs"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:cs="colorscheme_ignorable_uri"
                    xmlns:dxdlgn="http://schemas.devexpress.com/winfx/2008/xaml/dialogs/internal"
                    xmlns:dxdst="http://schemas.devexpress.com/winfx/2008/xaml/core/themekeys"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <dxdlgn:ContextMenuResources x:Key="ContextMenuResources"
                                 MenuBackground="#FFFFFF"
                                 MenuItemDisabledBackground="#FFFFFF"
                                 MenuItemDisabledForeground="#BBBBBB"
                                 MenuItemGlyphPadding="3, 0"
                                 MenuItemHoverBackground="#CFE7F8"
                                 MenuItemHoverForeground="#3C3C3C"
                                 MenuItemImagePadding="6,0"
                                 MenuItemNormalBackground="#FFFFFF"
                                 MenuItemNormalForeground="#3C3C3C"
                                 MenuItemPadding="3"
                                 MenuSeparatorBackground="#DDDDDD"
                                 MenuSeparatorMargin="9, 3"/>

</ResourceDictionary>