    <Style x:Key="{dxdst:DefaultStyleThemeKey FullName=DevExpress.Xpf.Charts.BorderlessGradientRangeBar2DModelControl}" TargetType="{x:Type local:BorderlessGradientRangeBar2DModelControl}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type local:BorderlessGradientRangeBar2DModelControl}">
                    <Border BorderThickness="0"
                            OpacityMask="{Binding OpacityMask}"
                            UseLayoutRounding="True">
                        <Border.Background>
                            <Binding Converter="{StaticResource brushOverlayConverter}"
                                     Path="PointColor"
                                     RelativeSource="{RelativeSource TemplatedParent}">
                                <Binding.ConverterParameter>
                                    <LinearGradientBrush EndPoint="0.5,1"
                                                         StartPoint="0.5,0.015625"
                                                         cs:id="LinearGradientBrush_0000">
                                        <GradientStop Color="#FF808080"
                                                      Offset="0"
                                                      cs:id="GradientStop_0000"/>
                                        <GradientStop Color="#FF4D4D4D"
                                                      Offset="1"
                                                      cs:id="GradientStop_0001"/>
                                    </LinearGradientBrush>
                                </Binding.ConverterParameter>
                            </Binding>
                        </Border.Background>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

