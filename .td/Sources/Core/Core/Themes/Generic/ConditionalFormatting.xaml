                    <GeometryGroup>
                        <RectangleGeometry Rect="0 0 1000 3"/>
                        <RectangleGeometry Rect="1000 3 1 1"/>
                    </GeometryGroup>
                </GeometryDrawing.Geometry>
            </GeometryDrawing>
        </DrawingBrush.Drawing>
    </DrawingBrush>

