                          Style="{Binding Path=(dxe:BaseEdit.OwnerEdit).(dxe:ActualPropertyProvider.Properties).IncrementButtonStyle}"
                          Visibility="{Binding Path=(dxe:BaseEdit.OwnerEdit).IsZoom, Converter={StaticResource BoolToVisibilityConverter}}"/>
            <RepeatButton x:Name="right"
                          Command="{Binding Path=(dxe:BaseEdit.OwnerEdit).IncrementLargeCommand}"
                          CommandParameter="SelectionEnd"
                          Grid.Column="1"
                          Grid.Row="1"
                          Style="{Binding Path=(dxe:BaseEdit.OwnerEdit).NearTrackStyle, RelativeSource={RelativeSource Self}}"/>
            <dxe:TrackBarEditThumb x:Name="PART_CenterThumb"
                                   Grid.Column="1"
                                   Grid.Row="3"
                                   Style="{DynamicResource {dxet:TrackBarEditThumbThemeKey ResourceKey=ScrollableCenterVerticalThumbStyle}}"/>
            <dxe:TrackBarEditThumb x:Name="PART_RightThumb"
                                   Grid.Column="1"
                                   Grid.Row="2"
                                   Style="{DynamicResource {dxet:TrackBarEditThumbThemeKey ResourceKey=ScrollableRightVerticalThumbStyle}}"/>
            <dxe:TrackBarEditThumb x:Name="PART_LeftThumb"
                                   Grid.Column="1"
                                   Grid.Row="4"
                                   Style="{DynamicResource {dxet:TrackBarEditThumbThemeKey ResourceKey=ScrollableLeftVerticalThumbStyle}}"/>
            <RepeatButton x:Name="left"
                          Command="{Binding Path=(dxe:BaseEdit.OwnerEdit).DecrementLargeCommand}"
                          CommandParameter="SelectionStart"
                          Grid.Column="1"
                          Grid.Row="5"
                          Style="{Binding Path=(dxe:BaseEdit.OwnerEdit).FarTrackStyle, RelativeSource={RelativeSource Self}}"/>
            <RepeatButton x:Name="PART_LeftStepButton"
                          Command="{Binding Path=(dxe:BaseEdit.OwnerEdit).DecrementSmallCommand}"
                          CommandParameter="SelectionRange"
                          Grid.Column="1"
                          Grid.Row="6"
                          Style="{Binding Path=(dxe:BaseEdit.OwnerEdit).(dxe:ActualPropertyProvider.Properties).DecrementButtonStyle}"
                          Visibility="{Binding Path=(dxe:BaseEdit.OwnerEdit).IsZoom, Converter={StaticResource BoolToVisibilityConverter}}"/>
            <!--BEGIN WPF-->
            <dxe:TrackBarEditTickItemsControl x:Name="PART_BottomTick"
                                              Grid.Column="2"
                                              Grid.Row="1"
                                              Grid.RowSpan="5"
                                              Placement="Right"
                                              ReservedSpace="{Binding Path=(dxe:RangeEditBaseInfo.LayoutInfo).ReservedSpace}"
                                              Style="{DynamicResource {dxet:TrackBarEditThemeKey ResourceKey=TickBarStyle}}"/>
            <!--END WPF-->
        </Grid>
        <!--BEGIN WPF-->
        <ControlTemplate.Triggers>
            <DataTrigger Binding="{Binding Path=(dxe:BaseEdit.OwnerEdit).TickPlacement}" Value="None">
                <Setter Property="Visibility"
                        TargetName="PART_BottomTick"
                        Value="Collapsed"/>
                <Setter Property="Visibility"
                        TargetName="PART_TopTick"
                        Value="Collapsed"/>
            </DataTrigger>
            <DataTrigger Binding="{Binding Path=(dxe:BaseEdit.OwnerEdit).TickPlacement}" Value="TopLeft">
                <Setter Property="Visibility"
                        TargetName="PART_BottomTick"
                        Value="Collapsed"/>
                <Setter Property="Visibility"
                        TargetName="PART_TopTick"
                        Value="Visible"/>
            </DataTrigger>
            <DataTrigger Binding="{Binding Path=(dxe:BaseEdit.OwnerEdit).TickPlacement}" Value="BottomRight">
                <Setter Property="Visibility"
                        TargetName="PART_BottomTick"
                        Value="Visible"/>
                <Setter Property="Visibility"
                        TargetName="PART_TopTick"
                        Value="Collapsed"/>
            </DataTrigger>
            <DataTrigger Binding="{Binding Path=(dxe:BaseEdit.OwnerEdit).TickPlacement}" Value="Both">
                <Setter Property="Visibility"
                        TargetName="PART_BottomTick"
                        Value="Visible"/>
                <Setter Property="Visibility"
                        TargetName="PART_TopTick"
                        Value="Visible"/>
            </DataTrigger>
        </ControlTemplate.Triggers>
        <!--END WPF-->
    </ControlTemplate>

    <ControlTemplate x:Key="{dxet:TrackBarEditThemeKey ResourceKey=PanelVerticalTemplate}" TargetType="dxe:RangeEditBasePanel">
        <Grid x:Name="root">
            <Grid.ColumnDefinitions>
                <ColumnDefinition/>
                <ColumnDefinition MinWidth="14"/>
                <ColumnDefinition/>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition x:Name="rightButton" Height="Auto"/>
                <RowDefinition x:Name="rightColumn" Height="{Binding Path=(dxe:RangeEditBaseInfo.LayoutInfo).RightSidePosition, Converter={StaticResource GridLengthConverter}}"/>
                <RowDefinition x:Name="centerColumn" Height="Auto"/>
                <RowDefinition x:Name="leftColumn" Height="{Binding Path=(dxe:RangeEditBaseInfo.LayoutInfo).LeftSidePosition, Converter={StaticResource GridLengthConverter}}"/>
                <RowDefinition x:Name="leftButton" Height="Auto"/>
            </Grid.RowDefinitions>
            <ContentControl x:Name="back"
                            dxei:ControlHelper.ShowFocusedState="{Binding Path=(dxe:BaseEdit.OwnerEdit).IsKeyboardFocusWithin}"
                            Grid.Column="1"
                            Grid.Row="1"
                            Grid.RowSpan="3"
                            Style="{DynamicResource {dxet:TrackBarEditThemeKey ResourceKey=VerticalBackgroundStyle}}"/>
            <!--BEGIN WPF-->
            <dxe:TrackBarEditTickItemsControl x:Name="PART_TopTick"
                                              Grid.Column="0"
                                              Grid.Row="1"
                                              Grid.RowSpan="3"
                                              Placement="Left"
                                              Style="{DynamicResource {dxet:TrackBarEditThemeKey ResourceKey=TickBarStyle}}"/>
            <!--END WPF-->
            <RepeatButton x:Name="PART_RightStepButton"
                          Command="{Binding Path=(dxe:BaseEdit.OwnerEdit).IncrementSmallCommand}"
                          Grid.Column="1"
                          Grid.Row="0"
                          Style="{Binding Path=(dxe:BaseEdit.OwnerEdit).(dxe:ActualPropertyProvider.Properties).IncrementButtonStyle}"
                          Visibility="{Binding Path=(dxe:BaseEdit.OwnerEdit).IsZoom, Converter={StaticResource BoolToVisibilityConverter}}"/>
            <RepeatButton x:Name="right"
                          Command="{Binding Path=(dxe:BaseEdit.OwnerEdit).IncrementLargeCommand}"
                          Grid.Column="1"
                          Grid.Row="1"
                          Style="{Binding Path=(dxe:BaseEdit.OwnerEdit).FarTrackStyle}"/>
            <dxe:TrackBarEditThumb x:Name="PART_Thumb"
                                   Grid.Column="1"
                                   Grid.Row="2"
                                   Panel.ZIndex="1"
                                   Style="{Binding Path=(dxe:BaseEdit.OwnerEdit).ThumbStyle}"/>
            <RepeatButton x:Name="left"
                          Command="{Binding Path=(dxe:BaseEdit.OwnerEdit).DecrementLargeCommand}"
                          Grid.Column="1"
                          Grid.Row="3"
                          Style="{Binding Path=(dxe:BaseEdit.OwnerEdit).NearTrackStyle}"/>
            <RepeatButton x:Name="PART_LeftStepButton"
                          Command="{Binding Path=(dxe:BaseEdit.OwnerEdit).DecrementSmallCommand}"
                          Grid.Column="1"
                          Grid.Row="4"
                          Style="{Binding Path=(dxe:BaseEdit.OwnerEdit).(dxe:ActualPropertyProvider.Properties).DecrementButtonStyle}"
                          Visibility="{Binding Path=(dxe:BaseEdit.OwnerEdit).IsZoom, Converter={StaticResource BoolToVisibilityConverter}}"/>
            <!--BEGIN WPF-->
            <dxe:TrackBarEditTickItemsControl x:Name="PART_BottomTick"
                                              Grid.Column="2"
                                              Grid.Row="1"
                                              Grid.RowSpan="3"
                                              Placement="Right"
                                              Style="{DynamicResource {dxet:TrackBarEditThemeKey ResourceKey=TickBarStyle}}"/>
            <!--END WPF-->
        </Grid>
        <!--BEGIN WPF-->
        <ControlTemplate.Triggers>
            <DataTrigger Binding="{Binding Path=(dxe:BaseEdit.OwnerEdit).TickPlacement}" Value="None">
                <Setter Property="Visibility"
                        TargetName="PART_BottomTick"
                        Value="Collapsed"/>
                <Setter Property="Visibility"
                        TargetName="PART_TopTick"
                        Value="Collapsed"/>
            </DataTrigger>
            <DataTrigger Binding="{Binding Path=(dxe:BaseEdit.OwnerEdit).TickPlacement}" Value="TopLeft">
                <Setter Property="Visibility"
                        TargetName="PART_BottomTick"
                        Value="Collapsed"/>
                <Setter Property="Visibility"
                        TargetName="PART_TopTick"
                        Value="Visible"/>
            </DataTrigger>
            <DataTrigger Binding="{Binding Path=(dxe:BaseEdit.OwnerEdit).TickPlacement}" Value="BottomRight">
                <Setter Property="Visibility"
                        TargetName="PART_BottomTick"
                        Value="Visible"/>
                <Setter Property="Visibility"
                        TargetName="PART_TopTick"
                        Value="Collapsed"/>
            </DataTrigger>
            <DataTrigger Binding="{Binding Path=(dxe:BaseEdit.OwnerEdit).TickPlacement}" Value="Both">
                <Setter Property="Visibility"
                        TargetName="PART_BottomTick"
                        Value="Visible"/>
                <Setter Property="Visibility"
                        TargetName="PART_TopTick"
                        Value="Visible"/>
            </DataTrigger>
        </ControlTemplate.Triggers>
        <!--END WPF-->
    </ControlTemplate>

    <ControlTemplate x:Key="{dxet:TrackBarEditThemeKey ResourceKey=Template}" TargetType="dxe:TrackBarEdit">
        <dxe:EditorControl x:Name="PART_Content" Template="{TemplateBinding ActualEditorControlTemplate}"/>
    </ControlTemplate>

    <ItemsPanelTemplate x:Key="{dxet:TrackBarEditThemeKey ResourceKey=ItemPanelTemplate}">
        <dxe:TrackBarEditTickPanel/>
    </ItemsPanelTemplate>

    <ControlTemplate x:Key="{dxet:TrackBarEditThemeKey ResourceKey=TickBarItemTemplate}" TargetType="{x:Type dxe:TrackBarEditTickItem}">
        <DockPanel x:Name="Panel"
                   Background="{TemplateBinding Background}"
                   LastChildFill="true"
                   Margin="{TemplateBinding Padding}">
            <Rectangle x:Name="Rectangle"
                       Fill="{TemplateBinding TextElement.Foreground}"
                       HorizontalAlignment="Center"
                       Opacity="{TemplateBinding TickOpacity}"/>
            <ContentPresenter x:Name="Presenter"
                              DockPanel.Dock="Bottom"
                              HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                              Margin="{TemplateBinding TickContentPadding}"
                              VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
        </DockPanel>
        <ControlTemplate.Triggers>
            <Trigger Property="DisplayMode" Value="Tick">
                <Setter Property="Visibility"
                        TargetName="Presenter"
                        Value="Collapsed"/>
            </Trigger>
            <Trigger Property="DisplayMode" Value="Text">
                <Setter Property="Visibility"
                        TargetName="Rectangle"
                        Value="Collapsed"/>
            </Trigger>
            <Trigger Property="Placement" Value="Top">
                <Setter Property="DockPanel.Dock"
                        TargetName="Rectangle"
                        Value="Bottom"/>
                <Setter Property="Width"
                        TargetName="Rectangle"
                        Value="1"/>
                <Setter Property="Height"
                        TargetName="Rectangle"
                        Value="{Binding Path=TickLength, RelativeSource={RelativeSource TemplatedParent}}"/>
            </Trigger>
            <Trigger Property="Placement" Value="Bottom">
                <Setter Property="DockPanel.Dock"
                        TargetName="Rectangle"
                        Value="Top"/>
                <Setter Property="Width"
                        TargetName="Rectangle"
                        Value="1"/>
                <Setter Property="Height"
                        TargetName="Rectangle"
                        Value="{Binding Path=TickLength, RelativeSource={RelativeSource TemplatedParent}}"/>
            </Trigger>
            <Trigger Property="Placement" Value="Left">
                <Setter Property="DockPanel.Dock"
                        TargetName="Rectangle"
                        Value="Right"/>
                <Setter Property="Width"
                        TargetName="Rectangle"
                        Value="{Binding Path=TickLength, RelativeSource={RelativeSource TemplatedParent}}"/>
                <Setter Property="Height"
                        TargetName="Rectangle"
                        Value="1"/>
            </Trigger>
            <Trigger Property="Placement" Value="Right">
                <Setter Property="DockPanel.Dock"
                        TargetName="Rectangle"
                        Value="Left"/>
                <Setter Property="Width"
                        TargetName="Rectangle"
                        Value="{Binding Path=TickLength, RelativeSource={RelativeSource TemplatedParent}}"/>
                <Setter Property="Height"
                        TargetName="Rectangle"
                        Value="1"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

</ResourceDictionary>